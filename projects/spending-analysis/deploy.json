{
  "namespace": "staging",
  "imageName": "djdplatform/spending-analysis-app",
  "containerPort": 3000,
  "env": [
    {
      "name": "MONGO_DB_USERNAME",
      "valueFrom": { "secretKeyRef": { "name": "spending-analysis-app-mongo", "key": "user" } }
    },
    {
      "name": "MONGO_DB_PASSWORD",
      "valueFrom": { "secretKeyRef": { "name": "spending-analysis-app-mongo", "key": "password" } }
    },
    {
      "name": "JWT_KEY",
      "valueFrom": { "secretKeyRef": { "name": "jwt", "key": "key" } }
    }
  ]
