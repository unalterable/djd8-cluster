{
  "namespace": "staging",
  "imageName": "djdplatform/app-spending-analysis",
  "containerPort": 3001,
  "env": [
    {
      "name": "MONGO_DB_USERNAME",
      "valueFrom": { "secretKeyRef": { "name": "app-spending-analysis-mongo", "key": "user" } }
    },
    {
      "name": "MONGO_DB_PASSWORD",
      "valueFrom": { "secretKeyRef": { "name": "app-spending-analysis-mongo", "key": "password" } }
    },
    {
      "name": "JWT_KEY",
      "valueFrom": { "secretKeyRef": { "name": "jwt", "key": "key" } }
    }
  ]
